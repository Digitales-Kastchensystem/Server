    "body-parser": "^1.20.2",
    "configparser": "^0.3.10",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.6.0",
    "nodemailer": "^6.9.4",
